        "webpack-merge": "^4.2.2"
    },
    "browserslist": [
        "> 0.5%",
        "not dead",
        "not IE < 11",
        "not op_mini all"
    ],
    "jest": {
        "collectCoverageFrom": [
            "<rootDir>/src/**/*{js,jsx,ts,tsx}"
        ],
        "coveragePathIgnorePatterns": [
            "<rootDir>/src/index.tsx"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/config/enzymeSetup.js"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|ico|woff|woff2|svg|ttf|eot)$": "<rootDir>/config/assetsTransformer.js",
            "^.+\\.(css|scss)$": "<rootDir>/config/assetsTransformer.js"
        },
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "testEnvironment": "jsdom"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run tsc && npm run lint",
            "pre-push": "npm test"
        }
    }
}
