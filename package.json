        "react-redux": "^8.0.2",
        "react-router": "^6.4.1",
        "react-router-dom": "^6.4.1",
        "redux": "^4.2.0",
        "redux-first-history": "^5.1.1",
        "redux-thunk": "^2.4.1",
        "reselect": "^4.1.6"
    },
