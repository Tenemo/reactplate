        "@types/http-server": "^0.12.1",
        "@types/jest": "^27.4.0",
        "@types/jsdom": "^16.2.14",
        "@types/mini-css-extract-plugin": "^2.4.0",
        "@types/node": "^17.0.5",
        "@types/npm": "^7.19.0",
        "@types/prettier": "^2.4.2",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
