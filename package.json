{
    "name": "reactplate",
    "version": "0.0.1",
    "author": "Piotr Piech <piotr@hadron.design>",
    "license": "UNLICENSED",
    "description": "Boilerplate | React + Redux + Jest + ðŸ”¥ reload | TypeScript parsed with Babel 7 | ESLint + @typescript-eslint",
    "scripts": {
        "start": "cross-env NODE_ENV=development run-p open:src test:watch",
        "prebuild": "npm run lint && npm test && npm run cleanDist",
        "build": "cross-env NODE_ENV=production babel-node config/build",
        "build:analyze": "cross-env NODE_ENV=production ANALYZE=true babel-node config/build",
        "cleanDist": "rimraf ./dist && mkdir dist",
        "open:dist": "cross-env webpack-dev-server --port 4000 --contentBase dist",
        "open:src": "cross-env PORT=3000 webpack-dev-server --hot --config config/webpack/webpack.dev.babel.js",
        "lint": "eslint src config --ext js,jsx,ts,tsx && stylelint **/*.scss **/*.jsx **/*.js **/*.ts **/*.tsx",
        "test": "cross-env NODE_ENV=test jest",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "npm run test -- --watch",
        "docs": "documentation build src/** -f html -o docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tenemo/reactplate.git"
    },
    "bugs": {
        "url": "https://github.com/Tenemo/reactplate/issues"
    },
    "homepage": "https://github.com/Tenemo/reactplate#readme",
    "dependencies": {
        "@babel/polyfill": "^7.2.5",
        "@types/styled-components": "^4.1.10",
