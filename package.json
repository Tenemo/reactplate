{
    "name": "reactplate",
    "version": "0.0.3",
    "author": "Piotr Piech <piotr@piech.dev>",
    "license": "MIT",
    "description": "TypeScript parsed with Babel 7 | React, Redux, Jest & ðŸ”¥ reload | ESLint + @typescript-eslint",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tenemo/reactplate.git"
    },
    "bugs": {
        "url": "https://github.com/Tenemo/reactplate/issues"
    },
    "homepage": "https://github.com/Tenemo/reactplate#readme",
    "scripts": {
        "start": "cross-env NODE_ENV=development npm run open:src",
        "prebuild": "npm run tsc && npm run lint && npm test && npm run cleanDist",
        "build": "cross-env NODE_ENV=production babel-node config/build",
        "build:analyze": "cross-env NODE_ENV=production ANALYZE=true babel-node config/build",
        "build:skip": "cross-env NODE_ENV=production babel-node config/build",
        "cleanDist": "rimraf ./dist && mkdir dist",
        "eslint": "eslint src config --ext js,jsx,ts,tsx",
        "eslint:fix": "eslint src config --ext js,jsx,ts,tsx --fix",
        "stylelint": "stylelint **/*.scss **/*.css",
        "lint": "npm run eslint && npm run stylelint",
        "open:dist": "cross-env webpack-dev-server --port 4000 --contentBase dist",
        "open:src": "cross-env PORT=3000 webpack-dev-server --hot --config config/webpack/webpack.dev.babel.js",
        "tsc": "tsc",
        "test": "cross-env NODE_ENV=test jest",
        "test:update": "npm run test -- --update",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "npm run test -- --watch"
    },
    "engines": {
        "node": ">=9"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "connected-react-router": "^6.8.0",
        "core-js": "^3.6.5",
        "history": "^4.10.1",
        "normalize.css": "^8.0.1",
        "npm": "^6.14.5",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-helmet-async": "^1.0.6",
        "react-redux": "^7.2.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0"
    },
