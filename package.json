{
    "name": "reactplate",
    "version": "1.0.2",
    "author": "Piotr Piech <piotr@piech.dev>",
    "license": "MIT",
    "description": "React, Redux, Jest & ðŸ”¥ reload | TypeScript parsed with Babel 7 | ESLint + @typescript-eslint",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tenemo/reactplate.git"
    },
    "bugs": {
        "url": "https://github.com/Tenemo/reactplate/issues"
    },
    "homepage": "https://github.com/Tenemo/reactplate#readme",
    "scripts": {
        "start": "cross-env NODE_ENV=development npm run open:src",
        "prebuild": "npm run lint && npm run tsc && npm test && npm run cleanDist",
        "build": "cross-env NODE_ENV=production babel-node config/build --extensions '.ts'",
        "build:analyze": "npm run cleanDist && cross-env NODE_ENV=production ANALYZE=true babel-node config/build --extensions '.ts'",
        "build:skip": "npm run cleanDist && cross-env NODE_ENV=production babel-node config/build --extensions '.ts'",
        "cleanDist": "rimraf ./dist && mkdir dist",
        "eslint": "eslint src config --ext js,jsx,ts,tsx",
        "eslint:fix": "eslint src config --ext js,jsx,ts,tsx --fix",
        "stylelint": "stylelint **/*.scss **/*.css",
        "lint": "npm run eslint && npm run stylelint",
        "open:dist": "http-server dist --port 4000  --proxy http://localhost:4000?",
        "open:src": "cross-env PORT=3000 webpack-dev-server --config config/webpack/webpack.dev.babel.ts",
        "postinstall": "husky install",
        "tsc": "tsc",
        "test": "cross-env NODE_ENV=test jest",
        "test:update": "npm run test -- --update",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "npm run test -- --watch"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run tsc",
            "pre-push": "npm test"
        }
    },
    "browserslist": [
        "> 0.5%",
        "not dead",
        "not IE > 0",
        "not op_mini all"
    ],
    "engines": {
        "node": ">=9"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "^7.13.1",
        "axios": "^0.21.1",
        "connected-react-router": "^6.9.1",
