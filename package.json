        "normalize.css": "^8.0.1",
        "npm": "^8.3.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet-async": "^1.2.2",
        "react-redux": "^7.2.6",
        "react-router": "^6.2.1",
