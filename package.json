    "description": "React, Redux Toolkit, Vite, Vitest & ðŸ”¥ reload | TypeScript | Flat ESLint config",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tenemo/reactplate.git"
    },
    "bugs": {
        "url": "https://github.com/Tenemo/reactplate/issues"
    },
    "homepage": "https://github.com/Tenemo/reactplate#readme",
    "scripts": {
        "dev": "cross-env NODE_ENV=development vite",
        "prebuild": "npm run lint && npm run tsc && npm test",
        "build": "cross-env NODE_ENV=production vite build",
        "build:skip": "cross-env NODE_ENV=production vite build",
        "build:analyze": "cross-env ANALYZE=true vite build",
        "eslint": "eslint . -c eslint.config.js",
        "eslint:fix": "eslint . --fix -c eslint.config.js",
        "stylelint": "stylelint **/*.scss **/*.css",
        "lint": "npm run eslint && npm run stylelint",
        "preview": "vite preview",
        "postinstall": "husky init",
        "tsc": "tsc",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "prepare": "husky"
    },
    "browserslist": [
        "> 0.5%",
        "not dead",
        "not IE > 0",
        "not op_mini all"
    ],
    "engines": {
        "node": ">=20.11.0"
    },
